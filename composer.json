{
    "require": {
        "php-di/php-di": "^6.1",
        "php-di/slim-bridge": "^3.0",
        "selective/array-reader": "^1.1",
        "selective/basepath": "^2",
        "selective/validation": "^2",
        "slim/psr7": "^1.4",
        "slim/slim": "4.*",
        "tuupola/slim-jwt-auth": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
